{
  "name": "webchat-with-conversations",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/ledvance", "destination": "/ledvance/index.html" },
    { "source": "/embed", "destination": "/" },
    { "source": "/(.*)", "destination": "/" }
  ]
}